void NhapN(long &n)
{
    printf("\nNhap Vao So Nguyen : ");
    scanf("%ld", &n);
}
void XuatN(long n)
{
    printf("So Ban Vua Nhap Vao La:%ld", n);
}
int XuLyDuLieu(int a[], long n)
{
    int i = 0;
    while (n != 0)
    {
        a[i] = n % 10;
        n = n / 10;
        i++;
    }
    return i;
}
void LietKeCacChuSo(int a[], long n)
{
    int s = XuLyDuLieu(a, n);
    printf("\nCac Chu So Cua So %ld La:", n);
    for (int k = s - 1; k >= 0; k--)
    {
        printf("%4d", a[k]);
    }
}
void ChuSoDauTien(int a[], long n)
{
    int s = XuLyDuLieu(a, n);
    printf("\nChu So Dau Tien Cua %ld La:%d", n, a[s - 1]);
}
void TongCacChuSo(int a[], long n)
{
    int s = XuLyDuLieu(a, n);
    int tong = 0;
    for (int k = s - 1; k >= 0; k--)
    {
        tong += a[k];
    }
    printf("\nTong Cac Chu So Cua So %ld La:%d", n, tong);
}
void ChuSoLonNhat(int a[], long n)
{
    int s = XuLyDuLieu(a, n);
    int max = a[0];
    for (int k = 1; k < s; k++)
    {
        if (a[k] > max)
        {
            max = a[k];
        }
    }
    printf("\nChu So Lon Nhat Cua So %ld La:%d", n, max);
}
void DemSoChuSo(int a[], long n)
{
    int s = XuLyDuLieu(a, n);
    printf("\nSo Luong Cac Chu So Cua So %ld La:%d", n, s);
}
void HoanVi(int &x, int &y)
{
    int temp = x;
    x = y;
    y = temp;
}
int KiemTraChuSoKhacNhauDoiMot(int a[], long n)
{
    int s = XuLyDuLieu(a, n);
    int dem = 0;
    for (int k = 0; k < s; k++)
    {
        for (int l = k + 1; l < s; l++)
        {
            if (a[k] > a[l])
            {
                HoanVi(a[k], a[l]);
            }
        }
    }
    printf("\nSo %ld Co Cac Chu So Khac Nhau Doi Mot Hay Khong ? ", n);
    for (int k = 1; k < s; k++)
    {
        if (a[k] != a[k - 1])
        {
            dem++;
        }
    }
    if (dem == s - 1)
    {
        printf("\nYES");
    }
    else
    {
        printf("\nNO");
    }
}
void MeNu()
{
    int a[MAX];
    long n, tieptuc;
quaylai:
    NhapN(n);
    printf("\n");
    XuatN(n);
    printf("\n");
    LietKeCacChuSo(a, n);
    printf("\n");
    ChuSoDauTien(a, n);
    printf("\n");
    TongCacChuSo(a, n);
    printf("\n");
    ChuSoLonNhat(a, n);
    printf("\n");
    DemSoChuSo(a, n);
    printf("\n");
    KiemTraChuSoKhacNhauDoiMot(a, n);
    printf("\n");
    printf("\nBan co muon tiep tuc thuc hien chuong trinh hay khong ? Neu co bam phim C,nguoc lai bam bat ky 1 phim nao khac de ket thuc!");
    tieptuc = getch();
    if (tieptuc == 'c' || tieptuc == 'C')
    {
        system("cls");
        goto quaylai;
    }
}
float tong_bai12(int n)
{
    float sum = 0;
    for (int i = 0; i <= n - 1; i++)
    {
        sum += (float)1 / (2 * i + 1);
    }
    return sum;
}
int KiemTraSoHoanThien(int n)
{
    int s = 0;
    int co = 1;
    for (int i = 1; i < n; i++)
    {
        if (n % i == 0)
            s = s + i;
    }
    if (s != n)
        co = 0;
    return co;
}
int Lietke_SNT(int n)
{
    if (n < 2)
    {
        return 0;
    }
    int i;
    int squareRoot = sqrt(n);
    for (i = 2; i <= squareRoot; i++)
    {
        if (n % i == 0)
        {
            return 0;
        }
    }
    return 1;
}
int ChinhPhuong(int n)
{
    int i = 1, j, dem = 0;
    int chinhphuong = 0;
    while (dem < n)
    {
        chinhphuong = 0;
        for (j = 1; j <= i; j++)
        {
            if (j * j == i)
            {
                chinhphuong = 1;
            }
        }
        if (chinhphuong == 1)
        {
            printf("\t %d", i);
            dem++;
        }
        i++;
    }
}
int SNT_cungnhau(int a, int b)
{
    int r;
    while (b)
        r = a % b, a = b, b = r;
    return (a == 1);
}
void Tinhphanso(int a, int b, int c, int d, int e, int f)
{
    float tu, mau, tu2, mau2;
    tu = (a * d * f) + (c * b * f) + (e * b * d);
    mau = b * d * f;
    tu2 = a * c * e;
    mau2 = b * d * f;
}
float Timucln(int a, int b)
{
    if (a == b)
        return a;
    if (a > b)
        return Timucln(a - b, b);
    else
        return Timucln(a, b - a);
}
void Nhapmang(float q[], int &o)
{
    do
    {
        printf("Nhap so phan tu cua mang: ");
        scanf("%d", &o);
    } while (o <= 0);
    for (int i = 0; i < o; i++)
    {
        printf("a[%d] = ", i);
        scanf("%f", &q[i]);
    }
}
void Xuatmang(float q[], int o)
{
    for (int i = 0; i < o; i++)
    {
        printf("\na[%d] = %4.2f", i, q[i]);
    }
}
float Tinhtongmang(float q[], int o)
{
    float s = 0;
    for (int i = 0; i < o; i++)
    {
        s += q[i];
    }
    return s;
}
float Tinhtrungbinh(float q[], int o)
{
    return (float)Tinhtongmang(q, o) / o;
}
float Tinhtongduong(float q[], int o)
{
    float s = 0;
    for (int i = 0; i < o; i++)
    {
        if (q[i] > 0)
            s += q[i];
    }
    return s;
}
void Daonguoc(float q[], int o)
{
    float tam;
    for (int i = 0; i <= o / 2; i++)
    {
        tam = q[i];
        q[i] = q[o - i - 1];
        q[o - i - 1] = tam;
    }
}
int Kiemtradoixung(float q[], int o)
{
    for (int i = 0; i <= o / 2; i++)
    {
        if (q[i] != q[o - i - 1])
            return -1;
    }
    return 0;
}
void Demsolanxuathien(float q[], int o)
{
    int dem = 1, max = 0;
    float sonhieunhat;
    for (int i = 0; i < o; i++)
    {
        for (int j = i + 1; j < o; j++)
        {
            if (q[i] == q[j])
            {
                dem++;
                if (dem > max)
                {
                    max = dem;
                    sonhieunhat = q[i];
                }
            }
            else
                dem = 1;
        }
    }
    printf("\nGia tri %.2f suat hien %d lan.", sonhieunhat, max);
}
int Kiemtramangtangdan(float q[], int o)
{
    for (int i = 0; i < o - 1; i++)
    {
        if (q[i] > q[i + 1])
            return 0;
    }
    return 1;
}
float TinhDienTichHinhTron(int bk)
{
    float r;
    r = bk * bk * 3.14;
    return r;
}
void inHello(int n)
{
    for (int i = 10; i <= n; i++)
        printf("Hello\n");
}
int Tong(int n)
{
    if (n == 1)
        return 1;
    else
    {
        return n + Tong(n - 1);
    }
}
int UCLN(int a, int b)
{
    while (a != b)
    {
        if (a > b)
            a = a - b;
        else
            b = b - a;
    }
    return a;
}
int KT_SNT(int n)
{
    int demuoc = 0;
    for (int i = 1; i <= n; i++)
        if (n % i == 0)
            demuoc++;
    if (demuoc == 2)
        return 1;
    return 0;
}
float LuyThua(float x, int n)
{
    if (n == 0)
        return 1;
    return x * LuyThua(x, n - 1);
}
float TinhDienTichTamGiac(int a, int b, int c)
{
    float r, p;
    p = (float)(a + b + c) / (float)2;
    r = sqrt(p * (p - a) * (p - b) * (p - c));
    return r;
}
int TongUoc(int n)
{
    int sum = 0;
    for (int i = 1; i <= n; i++)
    {
        if (n % i == 0)
        {
            sum += i;
        }
    }
    return sum;
}
int NgayThangNam(int thang, int nam)
{
    switch (thang)
    {
    case 1:
    case 3:
    case 5:
    case 7:
    case 8:
    case 10:
    case 12:
        return 31;
    case 2:
        return (nam % 4 == 0 && nam % 100 != 0 || nam % 400 == 0) ? 29 : 28;
    case 4:
    case 6:
    case 9:
    case 11:
        return 30;
    }
}
void hoanvi(int &a, int &b)
{
    int tam = a;
    a = b;
    b = tam;
}
void NhapMang1C_SoNguyen(int a[], int &n)
{
    do
    {
        printf("\nCho biet so phan tu cua mang: ");
        scanf("%d", &n);
    } while (n <= 0);
    for (int i = 0; i < n; i++)
    {
        printf("Gia tri phan tu a[%d] = ", i);
        scanf("%d", &a[i]);
    }
}
void XuatMang1C_SoNguyen(int a[], int n)
{
    for (int i = 0; i < n; i++)
        printf("%4d", a[i]);
}
void Xuat_SoChan(int a[], int n)
{
    for (int i = 0; i < n; i++)
    {
        if (a[i] % 2 == 0)
            printf("%4d", a[i]);
    }
}
int TongMang(int a[], int n)
{
    int s = 0;
    for (int i = 0; i < n; i++)
    {
        s += a[i];
    }
    return s;
}
int TongChan(int a[], int n)
{
    int s = 0;
    for (int i = 0; i < n; i++)
    {
        if (a[i] % 2 == 0)
            s += a[i];
    }
    return s;
}
float Giatri_trungbinh(int a[], int n)
{
    int sum = 0;
    for (int i = 0; i < n; i++)
    {
        sum += a[i];
    }
    return sum / n;
}
void Xuat_vtchan(int a[], int n)
{
    for (int i = 0; i < n; i++)
    {
        if (i % 2 == 0)
            printf("\n a[%d] = %d", i, a[i]);
    }
}
int Dem_soduong(int a[], int n)
{
    int dem = 0;
    for (int i = 0; i < n; i++)
    {
        if (a[i] > 0)
            dem++;
    }
    return dem;
    printf("\nSo phan tu duong: %d", dem);
}
void Vt_x(int a[], int n, int x)
{
    for (int i = 0; i < n; i++)
    {
        if (a[i] == x)
            printf(" a[%d]", i);
    }
}
int Solanxuathien(int a[], int n, int x)
{
    int dem = 0;
    for (int i = 0; i < n; i++)
    {
        if (a[i] == x)
        {
            dem++;
        }
    }
    return dem;
}
int max(int a[], int n)
{
    int max = a[0];
    for (int i = 0; i < n; i++)
    {
        if (a[i] > max)
            max = a[i];
    }
}
int min(int a[], int n)
{
    int min = a[0];
    for (int i = 0; i > n; i++)
    {
        if (a[i] < min)
            min = a[i];
    }
}
void Sapxeptangdan(int ab[], int n)
{
    for (int i = 0; i < n - 1; i++)
        for (int j = i + 1; j < n; j++)
            if (ab[i] > ab[j])
                HoanVi(ab[i], ab[j]);
}
void Nhap(int ab[], int &n)
{
    do
    {
        printf("\nCho biet so phan tu cua mang: ");
        scanf("%d", &n);
    } while (n <= 0);
    for (int i = 0; i < n; i++)
    {
        printf("Gia tri phan tu a[%d] = ", i);
        scanf("%d", &ab[i]);
    }
}
void Xuat(int ab[], int n)
{
    for (int i = 0; i < n; i++)
        printf("%4d", ab[i]);
}
void Themphantudau(int ab[], int &n, int x)
{
    for (int i = n; i > 0; i--)
        ab[i] = ab[i - 1];
    ab[0] = x;
    n++;
}
void Xoaphantudau(int ab[], int &n)
{
    for (int i = 0; i < n - 1; i++)
        ab[i] = ab[i + 1];
    n--;
}
void Duasokhongvedau(int ab[], int n)
{
    int j = 0;
    for (int i = 1; i < n; i++)
        if (ab[i] == 0)
        {
            HoanVi(ab[j], ab[i]);
            j++;
        }
}
void Lietkemangcontang(int ab[], int n)
{
    for (int i = 0; i < n; i++)
    {
        printf("%4d", ab[i]);
        if (ab[i] > ab[i + 1])
            printf("\n");
    }
}
void Taomangconle(int ab[], int na, int ba[], int &nb)
{
    nb = 0;
    for (int i = 0; i < na; i++)
        if (ab[i] % 2 != 0)
            ba[nb++] = ab[i];
}
void XuatSNT(int ab[], int n)
{
    int snt = 0;
    for (int i = 0; i <= n; i++)
    {
        for (int j = 1; j < ab[i]; j++)
        {
            if (ab[i] % j == 0)
                snt++;
        }
        if (snt == 1)
            printf("%4d ", ab[i]);
        snt = 0;
    }
}
void Xoa_vt(int ab[], int &n, int vt)
{
    for (int i = vt; i < n - 1; i++)
        ab[i] = ab[i + 1];

    n--;
}
void XoaPhanTuX(int ab[], int &n, int x)
{
    for (int i = 0; i < n; i++)
        if (x == ab[i])
        {
            Xoa_vt(ab, n, i);
            i--;
        }
}
void Themvt(int ab[], int &n, int &x, int &vt)
{
    for (int i = n; i >= vt; i--)
    {
        ab[i] = ab[i - 1];
    }
    ab[vt] = x;
    n++;
}
void Nhapchuoi(char s1[200], char s2[200])
{
    printf("\nnhap chuoi 1:");
    scanf("%d", s1);
    printf("\nnhap chuoi 2:");
    scanf("%d", s2);
}
void Xuatchuoi(char s1[200], char s2[200])
{
    printf("\nChuoi 1 = %s", s1);
    printf("\nChuoi 2 = %s", s2);
}
void SoSanhChuoiPhanBietHoaThuong(char s1[200], char s2[200])
{
    int kq = strcmp(s1, s2);
    if (kq > 0)
        printf("\nChuoi  %s lon hon %s", s1, s2);
    else if (kq < 0)
        printf("\nChuoi %s nho hon %s", s1, s2);
    else
        printf("\nChuoi %s bang %s", s1, s2);
}
void tolower(char s[100])
{
    for (int i = 0; i < strlen(s); i++)
        if (s[i] >= 'A' && s[i] <= 'Z')
            s[i] = s[i] + 32;
}
