void Nhapchuoi(char s1[200], char s2[200]);
void SoSanhChuoiPhanBietHoaThuong(char s1[200], char s2[200]);
void tolower(char s[100]);
int main()
{
    int chonbai, n, a, b, j, s = 0, gan, dem = 0, x, y, sum = 0, c, min, max, vao, ra, tieuthu, i, ngaunhien, o, luachon;
    int so, thang, nam, A[MAXSIZE], N, bk, ketqua = 0, S = 0, ab[maxsize], ba[maxsize], nb, vt;
    long r = 0;
    char s1[1000], s2[1000];
    float p = 0, u, e, f, d, toan, ly, hoa, van, anh, su, tb, tu, mau, tu2, mau2, q[maxside], v[maxside], x1, dtb;
    menu();
    do
    {
        printf("\n============================================================================\n");
        printf("\nChon bai:");
        scanf("%d", &chonbai);
        switch (chonbai)
        {
        case 1:
        {
            MeNu();
        }
        case 2:
        {
            n = 6;
            for (i = 1; i <= n; i++)
            {
                for (j = 1; j <= i; j++)
                    printf("* ");
                printf("\n");
            }
            break;
        }
        case 3:
        {
            n = 6;
            for (i = 1; i <= n; i++)
            {
                for (j = 1; j <= n - i; j++)
                    printf(" ");
                for (j = 1; j <= i; j++)
                    printf("* ");
                printf("\n");
            }
            break;
        }
        case 4:
        {
            n = 6;
            for (i = n; i >= 1; i--)
            {
                for (j = 1; j <= i; j++)
                    printf("* ");
                printf("\n");
            }
            break;
        }
        case 5:
        {
            do
            {
                printf("Nhap n: ");
                scanf("%d", &n);
            } while (n <= 0);
            for (i = 1; i <= n; i++)
            {
                s = s + pow(-1, i - 1) * i;
            }
            printf("\nTong %d", s);
            break;
        }
        case 6:
        {
            printf("nhap n = ");
            scanf("%d", &n);
            for (int i = 1; i <= n; i++)
            {
                p += (float)1 / i;
            }
            printf("S(%d) = %f", n, p);
            break;
        }
        case 7:
        {
            printf("nhap n = ");
            scanf("%d", &n);
            for (int i = 0; i <= n - 1; i++)
            {
                sum += (float)1 / (2 * i + 1);
            }
            printf("S(%d) = %f", n, sum);
            break;
        }
        case 8:
        {
            printf("\nNhap n = ");
            scanf("%d", &n);
            if (n < 2)
            {
                printf("\n%d khong phai so nguyen to", n);
                return 0;
            }
            int songuyento = 0;
            for (int i = 2; i <= sqrt(n); i++)
            {
                if (n % i == 0)
                {
                    songuyento++;
                }
            }
            if (songuyento == 0)
            {
                printf("\n%d la so nguyen to", n);
            }
            else
            {
                printf("\n%d khong phai so nguyen to", n);
            }
            break;
        }
        case 9:
        {
            do
            {
                printf("\nNhap N: ");
                scanf("%d", &N);
            } while (N < 0 && printf("\nLoi: n >= 0 !"));
            if (N == 0)
            {
                dem = 1;
            }
            gan = N;
            while (gan != 0)
            {
                if (gan % 2 == 0)
                {
                    dem = dem + 1;
                }
                gan = gan / 10;
            }
            printf("\nSo chu so chan cua %d la %d", N, dem);
            break;
        }
        case 10:
        {
            do
            {
                printf("Nhap a,b = ");
                scanf("%d%d", &a, &b);
            } while (a <= 0 || b <= 0);
            x = a;
            y = b;
            while (a != b)
            {
                if (a > b)
                    a -= b;
                else
                    b -= a;
            }
            printf("Uoc chung lon nhat la %d", a);
            printf("\nBoi chung nho nhat la %d", (x * y) / a);
            break;
        }
        case 11:
        {
            printf("\nNhap vao so nguyen duong n: ");
            scanf("%d", &n);
            int i = 1;
            while (i < n)
            {
                sum += i;
                i += 2;
            }
            printf("\nTong cac so tu 1 den %d la: %d", n, sum);
            break;
        }
        case 12:
        {
            printf("Nhap vao 3 so a, b, c: ");
            scanf("%f%f%f", &e, &f, &u);
            if (e == 0)
            {
                if (f == 0)
                {
                    if (u == 0)
                        printf("Phuong trinh co vo so nghiem!");
                    else
                        printf("Phuong trinh vo nghiem!");
                }
                else
                    printf("Phuong trinh co nghiem duy nhat la: %f", -u / f);
            }
            else
            {
                d = f * f - 4 * e * u;
                if (d < 0)
                    printf("Phuong trinh vo nghiem!!!");
                else if (d == 0)
                    printf("Phuong trinh co nghiem kep la: %f", -f / (2 * e));
                else
                    printf("Phuong trinh co 2 nghiem phan biet la: %f,%f", (-f + sqrt(d)) / (2 * e), (-f - sqrt(d)) / (2 * e));
            }
            break;
        }
        case 13:
        {
            printf("Nhap so thu nhat: ");
            scanf("%d", &a);
            printf("Nhap so thu hai: ");
            scanf("%d", &b);
            printf("Nhap so thu ba: ");
            scanf("%d", &c);
            min = max = a;
            if (b > max)
                max = b;
            if (c > max)
                max = c;
            printf("\nSo lon nhat la %d", max);
            if (b < min)
                min = b;
            if (c < min)
                min = c;
            printf("\nSo nho nhat la %d", min);
            break;
        }
        case 14:
        {
            printf("Nhap diem mon Toan: ");
            scanf("%f", &toan);
            printf("Nhap diem mon Ly: ");
            scanf("%f", &ly);
            printf("Nhap diem mon Hoa: ");
            scanf("%f", &hoa);
            printf("Nhap diem mon Van: ");
            scanf("%f", &van);
            printf("Nhap diem mon Anh: ");
            scanf("%f", &anh);
            printf("Nhap diem mon Su: ");
            scanf("%f", &su);
            tb = (toan + ly + hoa + van + anh + su) / 6;
            printf("Diem trung binh cua ban la %f", tb);
            if (tb >= 8.0)
            {
                if (toan >= 6.5 && ly >= 6.5 && hoa >= 6.5 && van >= 6.5 && anh >= 6.5 && su >= 6.5)
                    printf("\nXep loai: Gioi");
                if ((toan < 6.5 || ly < 6.6 || hoa < 6.5 || van < 6.5 || anh < 6.5 || su < 6.5) && (toan >= 5 && ly >= 5 && hoa >= 5 && van >= 5 && anh >= 5 && su >= 5))
                    printf("\nXep loai: Kha");
                if ((toan < 5 || ly < 5 || hoa < 5 || van < 5 || anh < 5 || su < 5) && (toan >= 3.5 && ly >= 3.5 && hoa >= 3.5 && van >= 3.5 && anh >= 3.5 && su >= 3.5))
                    printf("\nXep loai TB");
                if (toan < 3.5 || ly < 3.5 || hoa < 3.5 || van < 3.5 || anh < 3.5 || su < 3.5)
                    printf("\nXep loai Yeu");
            };
            if (tb >= 7 && tb < 8)
            {
                if (toan >= 5 && ly >= 5 && hoa >= 5 && van >= 5 && anh >= 5 && su >= 5)
                    printf("\nXep loai Kha");
                if ((toan < 5 || ly < 5 || hoa < 5 || van < 5 || anh < 5 || su < 5) && (toan >= 3.5 && ly >= 3.5 && hoa >= 3.5 && van >= 3.5 && anh >= 3.5 && su >= 3.5))
                    printf("\nXep loai TB");
                if (toan < 3.5 || ly < 3.5 || hoa < 3.5 || van < 3.5 || anh < 3.5 || su < 3.5)
                    printf("\nXep loai Yeu");
            };
            if (tb >= 5.0 && tb < 7)
            {
                if ((toan < 5 || ly < 5 || hoa < 5 || van < 5 || anh < 5 || su < 5) && (toan >= 3.5 && ly >= 3.5 && hoa >= 3.5 && van >= 3.5 && anh >= 3.5 && su >= 3.5))
                    printf("\nXep loai TB");
                if (toan < 3.5 || ly < 3.5 || hoa < 3.5 || van < 3.5 || anh < 3.5 || su < 3.5)
                    printf("\nXep loai Yeu");
            };
            if (tb < 5.0)
                printf("\nXep loai yeu");
            break;
        }
        case 15:
        {
            do
            {
                printf("Nhap gio vao quan: ");
                scanf("%d", &vao);
                if (vao < 10 || vao > 24)
                    printf("Gio Vao khong dung! Vui long nhap lai gio vao.\n");

            } while (vao < 10 || vao > 24);
            do
            {
                printf("Nhap gio ra ve: ");
                scanf("%d", &ra);
                if (ra > 24 || ra < vao)
                    printf("Gio Ra khong dung! Vui long nhap lai gio ra.\n");
            } while (ra > 24 || ra < vao);
            if (ra < 17)
                printf("So tien can thanh toan la %d dong", (ra - vao) * 20000);
            if (vao <= 17 && ra >= 17)
                printf("So tien can thanh toan la %d dong", (17 - vao) * 20000 + (ra - 17) * 45000);
            if (vao > 17)
                printf("So tien can thanh toan la %d dong", (ra - 17) * 45000);
            break;
        }
        case 16:
        {
            printf("Nhap muc tieu thu dien: ");
            scanf("%d", &tieuthu);
            if (tieuthu <= 100)
                printf("Tien dien thang nay la: %d $", tieuthu * 5);
            if (tieuthu > 100 && tieuthu <= 150)
                printf("Tien dien thang nay la: %d $", 500 + (tieuthu - 100) * 7);
            if (tieuthu > 150 && tieuthu <= 200)
                printf("Tien dien thang nay la: %d $", 850 + (tieuthu - 150) * 10);
            if (tieuthu > 200 && tieuthu <= 300)
                printf("Tien dien thang nay la: %d $", 1350 + (tieuthu - 200) * 15);
            if (tieuthu > 300)
                printf("Tien dien thang nay la: %d $", 2850 + tieuthu * 20);
            break;
        }
        case 17:
        {
            printf("\nTinh S(n)=1+1/3+1/5+...+1/2n+1");
            printf("\nnhap n = ");
            scanf("%d", &n);
            printf("       = %f", tong_bai12(n));
            break;
        }
        case 18:
        {
            printf("vui long nhap n: ");
            scanf("%d", &n);
            if (KiemTraSoHoanThien(n) == 1)
                printf("%d la so hoan thien", n);
            else
                printf("%d khong phai so hoan thien", n);
            break;
        }
        case 19:
        {
            printf("Nhap n = ");
            scanf("%d", &n);
            printf("%d so nguyen to dau tien la: \n", n);
            int dem = 0;
            int i = 2;
            while (dem < n)
            {
                if (Lietke_SNT(i))
                {
                    printf("%d   ", i);
                    dem++;
                }
                i++;
            }
            break;
        }
        case 20:
        {
            do
            {
                printf("Nhap n: ");
                scanf("%d", &n);
            } while (n <= 0);
            printf("Cac so chinh phuong la: ", ChinhPhuong(n));
            break;
        }
        case 21:
        {
            printf("-Tong cua 3 phan so tren: %.0f/%.0f\n", (float)tu / Timucln(tu, mau), (float)mau / Timucln(tu, mau));
            printf("-Tich cua 3 phan so tren: %.0f/%.0f\n", tu2 / Timucln(tu2, mau2), mau2 / Timucln(tu2, mau2));
            break;
        }
        case 22:
        {
            printf("\nNhap a = ");
            scanf("%d", &a);
            printf("\nNhap b = ");
            scanf("%d", &b);
            printf("\n%d\n\n\n", SNT_cungnhau(a, b));
            printf("\n(1 la SNT cung nhau, 0 la khong phai SNT cung nhau)");
            break;
        }
        case 23:
        {
            /*Nhapmang(q, o);
            srand(time('\0'));
            ngaunhien = 1 + rand() % 20;
            printf("\nNhap cac phan tu cua mang sinh tu dong voi n = %d\n", ngaunhien);
            for (int i = 0; i < ngaunhien; i++)
            {
                printf("a[%d] = ", i);
                scanf("%f", &v[i]);
            }
            break;*/
        }
        case 24:
        {
            printf("\nCac phan tu cua mang voi n nhap tu ban phim la: ");
            Xuatmang(q, o);
            printf("\nCac phan tu cua mang voi n sinh tu dong ngau nhien la: ");
            Xuatmang(v, ngaunhien);
            break;
        }
        case 25:
        {
            Nhapmang(q, o);
            printf("Tong cua mang vua nhap la: %.1f", Tinhtongmang(q, o));
            break;
        }
        case 26:
        {
            Nhapmang(q, o);
            printf("\nGia Tri Trung Binh Cua Mang La: %.2f", (float)Tinhtongmang(q, o) / o);
            break;
        }
        case 27:
        {
            Nhapmang(q, o);
            printf("\nTong cac gia tri duong cua mang la: %.2f", Tinhtongduong(q, o));
            break;
        }
        case 28:
        {
            Nhapmang(q, o);
            Daonguoc(q, o);
            printf("\nMang dao nguoc: ");
            Xuatmang(q, o);
            break;
        }
        case 29:
        {
            Nhapmang(q, o);
            Kiemtradoixung(q, o);
            if (Kiemtradoixung(q, o) == -1)
            {
                printf("\nKet Qua tra ve: %d", Kiemtradoixung(q, o));
            }
            else
                printf("\nMang vua nhap la mang doi xung.");
            break;
        }
        case 30:
        {
            Nhapmang(q, o);
            Demsolanxuathien(q, o);
            break;
        }
        case 31:
        {
            Nhapmang(q, o);
            Kiemtramangtangdan(q, o);
            if (Kiemtramangtangdan(q, o) == 0)
                printf("Mang vua nhap khong phai mang tang dan!!!");
            else
                printf("Mang vua nhap la mang tang dan.");
            break;
        }
        case 32:
        {
            printf("\nNhap diem trung binh mon NNLT:");
            scanf("%f", &dtb);
            if (dtb >= 4.0)
            {
                printf("\nDau");
            }
            else
            {
                printf("\nRot");
            }
            break;
        }
        case 33:
        {
            printf("nhap vao mot so nguyen");
            scanf("%d", &n);
            if (n % 2 == 0)
            {
                printf("%d la so chan", n);
            }
            else
            {
                printf("%d la so le", n);
            }
            break;
        }
        case 34:
        {
            printf("nhap nam:");
            scanf("%d", &n);
            if (n % 4 == 0 && n % 100 != 0 || n % 400 == 0)
            {
                printf("%d la nam nhuan", n);
            }
            else
            {
                printf("%d la nam khong nhuan", n);
            }
            break;
        }
        case 35:
        {
            float tl, gk, ck, dtb;
            printf("\nNhap diem giua ky cuoi ky:");
            scanf("%f", &tl);
            printf("\nNhap diem giua ky cuoi ky:");
